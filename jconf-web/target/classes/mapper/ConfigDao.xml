<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.wenbo2018.jconf.web.dao.ConfigDao">

    <resultMap id="BaseResultMap" type="com.github.webbo2018.jconf.web.dto.Config">
        <result column="Id" property="id" jdbcType="INTEGER" />
        <result column="Key" property="key" jdbcType="VARCHAR" />
        <result column="Value" property="value" jdbcType="VARCHAR" />
        <result column="Env" property="env" jdbcType="VARCHAR" />
        <result column="ProjectId" property="projectId" jdbcType="INTEGER" />
        <result column="UserName" property="userName" jdbcType="VARCHAR" />
        <result column="UserEmail" property="userEmail" jdbcType="VARCHAR" />
    </resultMap>

    <insert id="add" parameterType="com.github.webbo2018.jconf.web.dto.Config">
        insert into Config (
        Key,
        Value,
        ProjectId,UserName,UserEmail,DateTime
        )
        values (
        #{key},
        #{value},
        #{env},#{projectId},#{userName},#{userEmail},CURRENT_TIMESTAMP
        )
    </insert>

    <select id="list"  resultMap="BaseResultMap">
        SELECT * FROM Config
    </select>


</mapper>